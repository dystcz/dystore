{
    "name": "dystcz/dystore",
    "description": "Dystore Monorepo",
    "license": "MIT",
    "keywords": [
        "dystcz",
        "lunar",
        "lunar-api",
        "laravel",
        "modules",
        "php"
    ],
    "authors": [
        {
            "name": "Dystopia Developers",
            "email": "dev@dy.st"
        },
        {
            "name": "Jakub Theimer",
            "email": "jakub@dy.st",
            "role": "Developer"
        },
        {
            "name": "Marek LÅ‘rincz",
            "email": "marek@dy.st",
            "role": "Developer"
        },
        {
            "name": "Dystopia",
            "email": "dev@dy.st",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/dystcz/dystore",
    "require": {
        "php": "^8.2",
        "ext-redis": "*",
        "h4kuna/ares": "^3.0",
        "illuminate/support": "^11.0",
        "laravel-json-api/core": "4.1.0",
        "laravel-json-api/hashids": "^3.0",
        "laravel-json-api/laravel": "4.1.0",
        "laravel-json-api/non-eloquent": "^4.0",
        "laravel-notification-channels/discord": "^1.5",
        "lunarphp/lunar": "^1.0.0-beta.5",
        "lunarphp/stripe": "^1.0.0",
        "spatie/laravel-newsletter": "^5.1",
        "spatie/laravel-stripe-webhooks": "^3.6",
        "staudenmeir/eloquent-has-many-deep": "^1.20"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.0",
        "drewm/mailchimp-api": "^2.5",
        "getbrevo/brevo-php": "^1.0",
        "larastan/larastan": "^2.0",
        "laravel-json-api/testing": "^3.0",
        "laravel/pint": "^1.7",
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^9.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-faker": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "spatie/laravel-ray": "^1.32",
        "spatie/mailcoach-sdk-php": "^1.1",
        "symplify/monorepo-builder": "^11.2"
    },
    "replace": {
        "dystcz/dystore-api": "self.version",
        "dystcz/dystore-ares-module": "self.version",
        "dystcz/dystore-discord-module": "self.version",
        "dystcz/lunar-api": "self.version",
        "dystcz/lunar-api-newsletter": "self.version",
        "dystcz/lunar-api-product-notifications": "self.version",
        "dystcz/lunar-api-product-views": "self.version",
        "dystcz/lunar-api-reviews": "self.version",
        "dystcz/lunar-api-stripe-adapter": "self.version",
        "dystcz/lunar-rewards": "self.version"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Dystcz\\LunarApiNewsletter\\": "packages/newsletter/src",
            "Dystcz\\LunarApiProductNotification\\": "packages/product-notifications/src",
            "Dystcz\\LunarApiProductViews\\": "packages/product-views/src",
            "Dystcz\\LunarApiReviews\\": "packages/reviews/src",
            "Dystcz\\LunarApiStripeAdapter\\": "packages/stripe/src",
            "Dystcz\\LunarApi\\": "packages/api/src",
            "Dystcz\\LunarRewards\\": "packages/rewards/src",
            "Dystore\\Api\\": "packages/api/src",
            "Modules\\DystoreAres\\": "modules/ares/src/",
            "Modules\\DystoreAres\\Database\\Factories\\": "modules/ares/database/factories/",
            "Modules\\DystoreAres\\Database\\Seeders\\": "modules/ares/database/seeders/",
            "Modules\\DystoreDiscord\\": "modules/discord/src/",
            "Modules\\DystoreDiscord\\Database\\Factories\\": "modules/discord/database/factories/",
            "Modules\\DystoreDiscord\\Database\\Seeders\\": "modules/discord/database/seeders/"
        },
        "files": [
            "packages/api/autoload.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Dystcz\\LunarApiNewsletter\\Tests\\": "packages/newsletter/tests",
            "Dystcz\\LunarApiProductNotification\\Tests\\": "packages/product-notifications/tests",
            "Dystcz\\LunarApiProductViews\\Tests\\": "packages/product-views/tests",
            "Dystcz\\LunarApiReviews\\Tests\\": "packages/reviews/tests",
            "Dystcz\\LunarApiStripeAdapter\\Tests\\": "packages/stripe/tests",
            "Dystcz\\LunarApi\\Tests\\": "packages/api/tests",
            "Dystcz\\LunarRewards\\Tests\\": "packages/rewards/tests",
            "Dystore\\Api\\Tests\\": "packages/api/tests",
            "Dystore\\Tests\\Api\\": "tests/api",
            "Modules\\DystoreAres\\Tests\\": "modules/ares/tests/",
            "Modules\\DystoreDiscord\\Tests\\": "modules/discord/tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "aliases": {
                "LunarApi": "Dystcz\\LunarApi\\Facade\\LunarApi",
                "LunarApiNewsletter": "Dystcz\\LunarApiNewsletter\\LunarApiNewsletterFacade",
                "LunarApiProductViews": "Dystcz\\LunarApiProductViews\\LunarApiProductViewsFacade",
                "LunarReviews": "Dystcz\\LunarApiReviews\\Domain\\Reviews\\Facades\\LunarApiReview",
                "LunarRewards": "Dystcz\\LunarRewards\\Facade\\LunarRewards"
            },
            "providers": [
                "Dystcz\\LunarApiNewsletter\\LunarApiNewsletterServiceProvider",
                "Dystcz\\LunarApiProductNotification\\LunarApiProductNotificationServiceProvider",
                "Dystcz\\LunarApiProductViews\\LunarApiProductViewsServiceProvider",
                "Dystcz\\LunarApiReviews\\LunarReviewsServiceProvider",
                "Dystcz\\LunarApiStripeAdapter\\LunarApiStripeAdapterServiceProvider",
                "Dystcz\\LunarApi\\JsonApiServiceProvider",
                "Dystcz\\LunarApi\\LunarApiHashidsServiceProvider",
                "Dystcz\\LunarApi\\LunarApiServiceProvider",
                "Dystcz\\LunarRewards\\LunarRewardsServiceProvider",
                "Dystore\\Api\\JsonApiServiceProvider",
                "Dystore\\Api\\LunarApiHashidsServiceProvider",
                "Dystore\\Api\\LunarApiServiceProvider"
            ]
        }
    },
    "scripts": {
        "test:pest": "vendor/bin/pest",
        "test:phpstan": "vendor/bin/phpstan analyse",
        "format": "vendor/bin/pint",
        "test": [
            "@test:pest",
            "@test:phpstan"
        ]
    }
}
